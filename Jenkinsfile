pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building my jenkin..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing my jenkin..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying my jenkin....'
            }
        }
    }
}